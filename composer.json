{
    "name": "rhymedigital/contao_rhyme_ajax",
    "type": "symfony-bundle",
    "description": "An extension for Contao Open Source CMS that allows you to do simple AJAX calls on the front end.",
    "keywords": [
    	"ajax", 
    	"contao",
    	"symfony"
    ],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Rhyme Digital",
            "homepage": "http://rhyme.digital"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "contao/core-bundle": ">=4.0.0-dev",
        "symfony/symfony": ">=2.7"
    },
    "extra": {
        "contao": {
            "sources": {
                "ajax":"ajax",
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Rhyme\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}